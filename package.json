{
  "name": "live2d-widget",
  "version": "1.0.0",
  "description": "Add the Sseexxyyy live2d to webpages.",
  "main": "index.js",
  "scripts": {
    "inst:dev": "npm install -g webpack&&npm install -g commitizen&&npm install -g cz-conventional-changelog&&npm install -g conventional-changelog-cli&&npm install",
    "build:dev": "webpack --progress --colors --watch --config webpack.config.dev.js",
    "build:prod": "webpack --progress --colors --config webpack.config.prod.js",
    "commit": "git-cz",
    "changelog": "git pull&&conventional-changelog -i CHANGELOG.md -r 0 -s -u&&git add CHANGELOG.md&&git commit -m \"docs: update CHANGELOG\"&&git push&&git status",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xiazeyu/live2d-widget.js.git"
  },
